# Environment Variables for Physical AI & Humanoid Robotics Book

# Web Platform Configuration
PORT=3000
NODE_ENV=development

# ROS2 Configuration
ROS_DOMAIN_ID=0
ROS_LOG_DIR=./logs

# API Keys (for external services)
OPENAI_API_KEY=your_openai_api_key_here
WHISPER_API_KEY=your_whisper_api_key_here

# Database Configuration (if using)
DATABASE_URL=postgresql://localhost/humanoid_robotics_book

# RAG Service Configuration
VECTOR_DB_PATH=./vector_db
EMBEDDING_MODEL=sentence-transformers/all-MiniLM-L6-v2

# Pinecone Configuration
PINECONE_API_KEY=your_pinecone_api_key_here
PINECONE_ENVIRONMENT=your_pinecone_environment_here
PINECONE_INDEX_NAME=humanoid-robotics

# Supabase Configuration
SUPABASE_URL=your_supabase_url_here
SUPABASE_KEY=your_supabase_key_here

# Simulation Configuration
GAZEBO_MODEL_PATH=./ros2_ws/src/humanoid_control/humanoid_description/models
GAZEBO_RESOURCE_PATH=./ros2_ws/src/humanoid_control/humanoid_description